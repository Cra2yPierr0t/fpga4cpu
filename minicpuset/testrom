    %load/vec4 v0x558db18599d0_0;
    %load/vec4 v0x558db1808140_0;
    %pad/u 17;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x558db1859bc0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x558db1808140_0;
    %pad/u 17;
    %ix/vec4 4;
    %load/vec4a v0x558db1859bc0, 4;
    %assign/vec4 v0x558db1859c80_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x558db1830780;
T_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x558db18598e0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x558db18598e0_0;
    %cmpi/s 32768, 0, 32;
    %jmp/0xz T_1.1, 5;
    %pushi/vec4 0, 0, 16;
    %ix/getv/s 4, v0x558db18598e0_0;
    %store/vec4a v0x558db1859bc0, 4, 0;
    %load/vec4 v0x558db18598e0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x558db18598e0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %pushi/vec4 2084, 0, 16;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x558db1859bc0, 4, 0;
    %pushi/vec4 52, 0, 16;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x558db1859bc0, 4, 0;
    %end;
    .thread T_1;
    .scope S_0x558db1830600;
T_2 ;
